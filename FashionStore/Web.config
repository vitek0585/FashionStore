  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true">
      <add name="CultureHttpModule" type="FashionStore.Core.HttpModule.CultureHttpModule" />
      <add name="ErrorHandlerHttpModule" type="FashionStore.Core.HttpModule.ErrorHandlerHttpModule" />
    </modules>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
