  <connectionStrings>
    <add name="shopIdentity" connectionString="data source=localhost;initial catalog=ShopIdentity;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
    <add name="ShopContext" connectionString="data source=localhost;initial catalog=ShopDB;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
